config MEMUTILS_MEMORY_MANAGER
	bool "Memory manager"
	default n
	---help---
		Enable support for memory manager.

if MEMUTILS_MEMORY_MANAGER
config NUM_FIXED_AREA_FENCES
	int "area fence num"
	default 0

endif
